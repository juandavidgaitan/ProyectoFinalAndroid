<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".MenuVeterinaria">

    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:orientation="vertical"
        tools:layout_editor_absoluteX="-16dp"
        tools:layout_editor_absoluteY="-131dp">

        <Space
            android:layout_width="match_parent"
            android:layout_height="197dp" />

        <TextView
            android:id="@+id/textView6"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:gravity="center"
            android:text="Menu veterinaria"
            android:textSize="25dp" />

        <Space
            android:layout_width="match_parent"
            android:layout_height="28dp" />

        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="65dp"
            android:orientation="horizontal">

            <Button
                android:id="@+id/button30"
                android:layout_width="174dp"
                android:layout_height="wrap_content"
                android:layout_marginLeft="24dp"
                android:layout_marginStart="14dp"

                android:background="@drawable/borde_redondo"
                android:onClick="GestionarServicios"
                android:text="Gestionar servicios" />

            <Button
                android:id="@+id/button29"
                android:layout_width="174dp"
                android:layout_height="wrap_content"
                android:layout_marginLeft="24dp"
                android:layout_marginStart="14dp"

                android:background="@drawable/borde_redondo"
                android:onClick="GestionarVeterinario"
                android:text="Gestionar veterinario" />
        </LinearLayout>

        <Space
            android:layout_width="match_parent"
            android:layout_height="18dp" />

        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="74dp"
            android:orientation="horizontal">

            <Button
                android:id="@+id/button32"
                android:layout_width="174dp"
                android:layout_height="wrap_content"
                android:layout_marginLeft="24dp"
                android:layout_marginStart="14dp"

                android:background="@drawable/borde_redondo"
                android:text="Lista de citas" />

            <Button
                android:id="@+id/button31"
                android:layout_width="174dp"
                android:layout_height="wrap_content"
                android:layout_marginStart="14dp"
                android:layout_marginLeft="24dp"
                android:layout_marginTop="-8dp"

                android:background="@drawable/borde_redondo"
                android:onClick="GestionarLocalizacion"
                android:text="Gestionar localizacion" />
        </LinearLayout>

    </LinearLayout>
</androidx.constraintlayout.widget.ConstraintLayout>